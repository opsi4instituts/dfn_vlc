[visual] # Visualizer filter

# Effects list (Text)
#effect-list=spectrum

# Video width (Ganzzahl)
#effect-width=800

# Video height (Ganzzahl)
#effect-height=500

# FFT window (Text)
#effect-fft-window=flat

# Kaiser window parameter (Fließkommazahl)
#effect-kaiser-param=3.000000

# Show 80 bands instead of 20 (boolsche (logisch))
#visual-80-bands=1

# Draw peaks in the analyzer (boolsche (logisch))
#visual-peaks=1

# Enable original graphic spectrum (boolsche (logisch))
#spect-show-original=0

# Draw the base of the bands (boolsche (logisch))
#spect-show-base=1

# Base pixel radius (Ganzzahl)
#spect-radius=42

# Spectral sections (Ganzzahl)
#spect-sections=3

# V-plane color (Ganzzahl)
#spect-color=80

# Draw bands in the spectrometer (boolsche (logisch))
#spect-show-bands=1

# Show 80 bands instead of 20 (boolsche (logisch))
#spect-80-bands=1

# Number of blank pixels between bands. (Ganzzahl)
#spect-separ=1

# Amplification (Ganzzahl)
#spect-amp=8

